                    <input class="form-check-input" type="radio" value="candidate_2" id="candidate_2"
                    name="candidates">
                    <label class="form-check-label" for="candidate_2">
                      SELECT
                    </label>
                  </div>
                </div>
                <hr class="vertical dark" />
              </div>
            <% } %>
            <% if(index === 2) { %>
              <div class="col-md-4 position-relative ">
